{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "appInteractionLog.json",
  "definitions": {
    "AppInteractionLog": {
      "type": "object",
      "properties": {
        "user": {
          "type": "string"
        },
        "logType": {
          "type": "string",
          "enum": [
            "openIntegrationDartBlockPad",
            "openLighthouse",
            "openCourseBeaconResources",
            "openCourseAchievements",
            "viewTopicResources",
            "openCourseLeaderboard",
            "openPatronSelectionPage",
            "openCoursePatronLeaderboard"
          ]
        },
        "content": {
          "type": "string"
        },
        "interactionTimestamp": {
          "type": "string",
          "format": "date-time"
        },
        "createdAt": {
          "type": "string",
          "format": "date-time"
        },
        "updatedAt": {
          "type": "string",
          "format": "date-time"
        }
      },
      "required": [
        "user",
        "logType",
        "interactionTimestamp"
      ],
      "additionalProperties": false
    }
  }
}
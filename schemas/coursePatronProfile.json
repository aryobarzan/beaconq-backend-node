{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "coursePatronProfile.json",
  "definitions": {
    "CoursePatronProfile": {
      "type": "object",
      "properties": {
        "course": {
          "type": "string"
        },
        "patron": {
          "type": "string"
        },
        "contributionCount": {
          "type": "number"
        },
        "score": {
          "type": "number"
        },
        "createdAt": {
          "type": "string",
          "format": "date-time"
        },
        "updatedAt": {
          "type": "string",
          "format": "date-time"
        }
      },
      "required": [
        "patron",
        "contributionCount",
        "score"
      ],
      "additionalProperties": false
    }
  }
}